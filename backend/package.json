{
  "name": "boetos-backend",
  "version": "1.0.0",
  "description": "Backend for Boetos burnout tracking engine",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "rimraf dist && tsc && copyfiles -u 1 src/**/*.js src/**/*.json src/**/*.graphql src/**/*.gql dist/",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "migrate": "ts-node src/migrate.ts",
    "migration:run": "ts-node -r dotenv/config ./node_modules/typeorm/cli-ts-node-commonjs.js migration:run -d src/data-source.ts",
    "migration:revert": "ts-node -r dotenv/config ./node_modules/typeorm/cli-ts-node-commonjs.js migration:revert -d src/data-source.ts",
    "clean-start": "ts-node src/clean-start.ts",
    "clean-dev": "nodemon --exec ts-node src/clean-start.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@langchain/google-genai": "^0.2.13",
    "@prisma/client": "^6.11.1",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "elevenlabs": "^1.59.0",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "form-data": "^4.0.0",
    "googleapis": "^100.0.0",
    "jsonwebtoken": "^9.0.0",
    "langchain": "^0.3.29",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.2",
    "react-onesignal": "^3.2.3",
    "reflect-metadata": "^0.1.13",
    "resend": "^4.6.0",
    "typeorm": "^0.3.25",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.4",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pg": "^8.11.11",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
