{
  "name": "boetos-backend",
  "version": "1.0.0",
  "description": "Backend for Boetos burnout tracking engine",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index.js",
    "build": "tsc",
    "start": "node src/index.js",
    "typeorm": "node -r dotenv/config --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate": "node src/migrate.js",
    "migration:run": "node -r dotenv/config ./node_modules/typeorm/cli.js migration:run -d src/data-source.js",
    "migration:revert": "node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert -d src/data-source.js",
    "clean-start": "node src/clean-start.js",
    "clean-dev": "nodemon src/clean-start.js"
  },
  "dependencies": {
    "@langchain/google-genai": "^0.2.13",
    "@prisma/client": "^6.11.1",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "elevenlabs": "^1.59.0",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "form-data": "^4.0.0",
    "googleapis": "^100.0.0",
    "jsonwebtoken": "^9.0.0",
    "langchain": "^0.3.29",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.2",
    "react-onesignal": "^3.2.3",
    "reflect-metadata": "^0.1.13",
    "resend": "^4.6.0",
    "typeorm": "^0.3.25",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.22",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.19.2",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  }
}
